{
  "name": "ispct",
  "version": "2025.4.3",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "env INLINE_RUNTIME_CHUNK=false react-app-rewired build",
    "prettier:write": "prettier --write .",
    "eslint:check": "eslint . --ext .ts,.js,.json",
    "eslint:fix": "eslint . --ext .ts,.js,.json --fix",
    "lint:types": "tsc",
    "lint:check": "npm run eslint:check && npm run lint:types",
    "lint:fix": "npm run prettier:write && npm run eslint:fix && npm run lint:types"
  },
  "dependencies": {
    "@types/node": "22.13.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "customize-cra": "1.0.0",
    "dotenv": "16.4.7",
    "eslint": "9.20.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "prettier": "3.5.1",
    "react": "19.0.0",
    "react-app-rewired": "2.2.1",
    "react-dom": "19.0.0",
    "react-router-dom": "7.2.0",
    "react-scripts": "5.0.1",
    "react-tooltip": "5.28.0",
    "typescript": "4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "ts-loader": "9.5.2",
    "webpack": "5.98.0",
    "webpack-cli": "6.0.1"
  }
}
